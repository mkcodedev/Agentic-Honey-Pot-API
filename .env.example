# Environment Configuration Template
# Copy this file to .env and fill in your actual values

# LLM Provider (gemini | openai | openrouter)
LLM_PROVIDER=gemini

# API key for your LLM provider
LLM_API_KEY=your-gemini-api-key-here

# Honeypot API authentication key (sent as x-api-key header)
HONEYPOT_API_KEY=your-honeypot-api-key-here

# Backend URL (for local development)
BACKEND_URL=http://localhost:8000

# Optional: OpenRouter or other AI agent API key
AI_AGENT_API_KEY=your-openrouter-key-here

# Server port (Railway sets this automatically)
PORT=8000
