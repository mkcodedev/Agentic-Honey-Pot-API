# Environment Configuration
# Copy this file to .env and fill in your values

# API Authentication (required)
# The key your API expects in the x-api-key header
HONEYPOT_API_KEY=your-secret-api-key-here

# AI Agent API Key (OpenRouter or similar)
AI_AGENT_API_KEY=your-openrouter-api-key-here

# LLM Provider Configuration
# Supported: gemini
# Leave empty to use rule-based detection only
LLM_PROVIDER=gemini
LLM_API_KEY=your-gemini-api-key-here

# Server Configuration
PORT=8000
